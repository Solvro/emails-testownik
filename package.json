  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@eslint/js": "^9.39.1",
    "@solvro/config": "^2.0.7",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.5",
